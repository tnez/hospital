%h2 Patient List
%ul.patient-list
  - @patients.each do |patient|
    %li.patient-item
      = patient.name_last + ', ' + patient.name_first
      = link_to "view", patient_path(patient)
      |
      = link_to "edit", edit_patient_path(patient)
      |
      = link_to "delete", patient_path(patient), method: :delete
